<!-- Registrierung -->
<h1>Registrieren</h1>

<form action="/register" enctype="multipart/form-data" method="post">
  Voller Name:
  <input type="text" name="vollername" required /><br />
  Benutzername:
  <input type="text" name="benutzername" required /><br />
  Passwort:
  <input type="password" name="passwort" required /><br />
  <input type="submit" />
</form>